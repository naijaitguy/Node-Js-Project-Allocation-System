<%- include ("header")  %>
<%- include ("Navbar")  %>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-3 col-md-2 sidebar">
          <%- include ("sidebar")  %>
        </div>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <h1 class="page-header text-primary">Edit Project Details</h1>
          <div class="col-md-5 well col-md-offset-3">  
          
     <div class=" alert-danger">
	  
	
		
	</div>
	 
	 <div class=" alert-success">
	
	 </div>
         
    
     <form action="/Project/Edit_Project" method="post" role="form"> 
          <div class="form-group">
          	<label> Project Topic :</label>
          	
     <input type="text" name="topic" required class="form-control" value="<%= Data[0].Project_Topic %>">

  
          	
          </div>
          <input type="hidden"  required name="id" class="form-control" value="<%= Data[0].ID %>">
           
            <div class="form-group">
          	<label> Area Of Knowledge :</label>
          	
 <input type="text" name="area" class="form-control" required  value="<%= Data[0].  Area_Of_Knowledge %> " >
          	
          </div>
          
          
            <div class="form-group">
          	<label> Assign Supervisor :</label>
          	
 <input type="text" name="supervisor" required class="form-control" value="<%= Data[0].supervisor %>" >
          	
          </div>
          
 <div class="form-group">
          	<label> Program :</label>
          	
   <select class="form-control" required name="program">  <%= Data[0].Program %> 
    <option value="<%= Data[0].Program %>"> </option> 
      <option value="BSC"> B.Sc </option>
         <option value="MSC"> M.Sc </option>
          	<option value="PGD"> P.gd </option>
              </select>
          	
          </div>
          
          
   <div class="form-group">
     <input type="submit" name="Submit_Project" value="Save Project Details" class="btn btn-block btn-success">
          	
	  </div>
         
         </form>
          
          </div>
         
      </div>
    </div>

		
          
<div class="row">
  <%- include ("footer")  %>
</div>